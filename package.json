{
  "name": "resourceflow",
  "version": "1.0.1",
  "description": "**Slack-First Resourcing & Budget Tracker for Use All Five**",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:api": "esbuild src/api/server.ts --bundle --platform=node --target=node20 --outfile=dist/api/server.js --packages=external",
    "build:slack": "esbuild src/slack/index.ts --bundle --platform=node --target=node20 --outfile=dist/slack/index.js --packages=external",
    "preview": "vite preview --host 0.0.0.0 --port ${PORT:-4173}",
    "slack": "tsx src/slack/index.ts",
    "slack:dev": "tsx watch src/slack/index.ts",
    "slack:start": "node dist/slack/index.js",
    "api:start": "node minimal-server.js",
    "api:dev": "tsx watch src/api/server.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.9",
    "@google/generative-ai": "^0.24.1",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@mui/x-data-grid": "^8.24.0",
    "@slack/bolt": "^4.6.0",
    "@supabase/supabase-js": "^2.90.1",
    "canvas-confetti": "^1.9.4",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "framer-motion": "^12.29.2",
    "googleapis": "^170.0.0",
    "helmet": "^8.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.12.0",
    "recharts": "^3.7.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/canvas-confetti": "^1.9.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.4",
    "esbuild": "^0.27.2",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^6.3.5"
  }
}
